{
  "manifest_version": 3,
  "name": "WebAssign Extractor",
  "version": "1.0",
  "description": "Extract and convert WebAssign questions for easy copy-pasting.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  }
}
